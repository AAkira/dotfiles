[user]
	name = AAkira
	email = developer.a.akira@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	editor = nvim -c \"set fenc=utf-8\"
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
	ui = true
[push]
	default = simple
[alias]
	co = checkout
	cb = checkout -b
	cob = !git checkout $(git branch | sed 's/*//g' | sed 's/ //g' | peco)
	br = branch
	st = status
	di = diff
	c = commit -m
	cc = commit
	ca = commit --amend
	a = add .
	aa = add --all
	p = push
	pl = pull
	me = merge
	m = master
	o = origin
	u = upstream
	cf = !git ls-files -u | cut -f 2 | sort -u
	delete-merged = !"git branch --merged | grep -v '*' | xargs -I % git branch -d %"
[secrets]
	providers = git secrets --aws-provider
	patterns = [A-Z0-9]{20}
	patterns = (\"|')?(AWS|aws|Aws)?_?(SECRET|secret|Secret)?_?(ACCESS|access|Access)?_?(KEY|key|Key)(\"|')?\\s*(:|=>|=)\\s*(\"|')?[A-Za-z0-9/\\+=]{40}(\"|')?
	patterns = (\"|')?(AWS|aws|Aws)?_?(ACCOUNT|account|Account)_?(ID|id|Id)?(\"|')?\\s*(:|=>|=)\\s*(\"|')?[0-9]{4}\\-?[0-9]{4}\\-?[0-9]{4}(\"|')?
	allowed = AKIAIOSFODNN7EXAMPLE
	allowed = wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY
[include]
    path = ~/.gitconfig.local
[commit]
	template = ~/.stCommitMsg
