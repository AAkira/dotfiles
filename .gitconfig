[user]
	name = A.Akira
	email = developer.a.akira@gmail.com
[core]
	excludesfile = /Users/akira/.gitignore_global
	editor = /usr/bin/vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
	ui = true
[push]
	default = simple
[alias]
	co = checkout
	cb = checkout -b
	br = branch
	st = status
	di = diff
	c = commit -m
	cc = commit
	ca = commit --amend
	a = add .
	aa = add --all
	p = push
